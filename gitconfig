[github]
    user = vesln
[alias]
    pr = "!f() { git fetch origin refs/pull/$1/head:pr/$1; } ; f"
    unstage = reset HEAD
    track = "!f(){ branch=$(git name-rev --name-only HEAD); cmd=\"git branch --set-upstream-to=${1:-origin}/${2:-$branch} $branch\"; echo $cmd; $cmd; }; f"
[apply]
    whitespace = warn
[color]
    diff = auto
    status = auto
    branch = auto
    ui = true
[help]
    autocorrect = 0
[status]
    submodule = 1
[merge]
    tool = vimdiff
    log = true
    prompt = false
[difftool]
    prompt = false
[push]
    default = tracking
[core]
    excludesfile = /Users/vesln/.gitignore_global
    trustctime = false
[branch]
    autosetuprebase = always
